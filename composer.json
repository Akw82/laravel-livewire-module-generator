{
    "name": "akw82/laravel-livewire-module-generator",
    "description": "create a module containing everything you want for a starter module.",
    "license": "MIT",
    "keywords": ["laravel", "livewire", "laravel livewire module generator"],
    "autoload": {
        "psr-4": {
            "Akw82\\LaravelLivewireModuleGenerator\\": "src/"
        }
    },
    "extra": {
          "laravel": {
		"providers": ["Akw82\\LaravelLivewireModuleGenerator\\GenerateModuleServiceProvider"]
	}
    },
    "authors": [
        {
            "name": "Mohamed Akram",
            "email": "akw82@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.2",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.5"
    }
}
